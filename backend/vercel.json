{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/**/*",
          "node_modules/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
